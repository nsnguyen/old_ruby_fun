<h1>Listing Users</h1>
<table>
<tr>
<th>Username</th>
<th>Password</th>
<th>Pin</th>
<th>Question 1</th>
<th>Question 2</th>
<th>Email</th>
<th></th>
<th></th>
</tr>

<% @users.each do |user| %>
<tr>
  <td><%= user.username %></td>
  <td><%= user.password %></td>
  <td><%= user.pin %></td>
  <td><%= user.question1 %></td>
  <td><%= user.question2 %></td>
  <td><%= user.email %></td>
  <td><%= link_to 'Show', user %></td>
  <td><%= link_to 'Edit', edit_user_path(user) %></td>
  <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
</tr>
<% end %>
</table>